{
  "name": "gistprint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run css:build && vite",
    "build": "npm run css:build && vite build",
    "preview": "vite preview",
    "css:watch": "tailwindcss -i ./input.css -o ./styles.css --watch",
    "css:build": "tailwindcss -i ./input.css -o ./styles.css --minify"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "lucide-react": "^0.563.0",
    "remark-gfm": "^4.0.1",
    "rehype-highlight": "^7.0.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}